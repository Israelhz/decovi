<p id="notice"><%= notice %></p>

<h1>Direcciones Entregas</h1>

<table>
  <thead>
    <tr>
      <th>Estado</th>
      <th>Municipio</th>
      <th>Colonia</th>
      <th>Numero</th>
      <th>Cp</th>
      <th>Telefono</th>
      <th>Notas</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @direcciones_entregas.each do |direcciones_entrega| %>
      <tr>
        <td><%= direcciones_entrega.estado %></td>
        <td><%= direcciones_entrega.municipio %></td>
        <td><%= direcciones_entrega.colonia %></td>
        <td><%= direcciones_entrega.numero %></td>
        <td><%= direcciones_entrega.cp %></td>
        <td><%= direcciones_entrega.telefono %></td>
        <td><%= direcciones_entrega.notas %></td>
        <td><%= link_to 'Show', direcciones_entrega %></td>
        <td><%= link_to 'Edit', edit_direcciones_entrega_path(direcciones_entrega) %></td>
        <td><%= link_to 'Destroy', direcciones_entrega, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Direcciones Entrega', new_direcciones_entrega_path %>

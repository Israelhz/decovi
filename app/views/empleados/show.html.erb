<div class="divide80"></div>
<div class="panel panel-default">
  <div class="panel-heading"><h2>Ver empleado</h2></div>
  <div class="panel-body">
    <p id="notice"><%= notice %></p>

    <p>
      <strong>Nombre:</strong>
      <%= @empleado.nombre %>
    </p>

    <p>
      <strong>Apellidom:</strong>
      <%= @empleado.apellidoM %>
    </p>

    <p>
      <strong>Apellidop:</strong>
      <%= @empleado.apellidoP %>
    </p>

    <p>
      <strong>Correo:</strong>
      <%= @empleado.correo %>
    </p>

    <p>
      <strong>Password:</strong>
      <%= @empleado.password %>
    </p>

    <%= link_to 'Edit', edit_empleado_path(@empleado), class:'btn btn-default' %> 
    <%= link_to 'Back', empleados_path, class:'btn btn-default'  %>
  </div>
</div>